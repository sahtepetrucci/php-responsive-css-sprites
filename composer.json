{
    "name": "sahtepetrucci/php-retina-css-sprites",
    "description": "A quick and dirty CSS sprites generator. Built to be used with Laravel models.",
    "type": "library",
    "require": {
        "php": ">=7.2",
        "orbitale/imagemagick-php": ">=3.0"
    },
    "require-dev": {
        "phpunit/phpunit": ">=8.0"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Serdar Cevher",
            "email": "scevher@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Sprites\\": "src"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Sprites\\SpritesServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "phpunit tests"
    }

}
